    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "katex": "^0.13.18",
    "mathjax3": "^3.0.0-beta.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "vis-data": "^7.1.2",
    "vis-network": "^9.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
